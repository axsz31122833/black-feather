{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "scripts/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "scripts/dist" }
    }
  ],
  "buildCommand": "npm run build --prefix scripts",
  "outputDirectory": "scripts/dist",
  "installCommand": "npm ci --prefix scripts"
}
