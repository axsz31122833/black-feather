{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/static-build" }],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "VITE_SUPABASE_URL": "",
    "VITE_SUPABASE_ANON_KEY": "",
    "VITE_GOOGLE_MAPS_API_KEY": "",
    "VITE_PUBLIC_APP_URL": "",
    "VITE_MAPBOX_ACCESS_TOKEN": "",
    "VITE_MAP_PROVIDER": "google"
    
  }
}